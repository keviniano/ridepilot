<% if @target_trip_ids.any? %>
  <% if @prev_run.present? %>
    var prev_run_id = "<%= @prev_run.id %>";
    $('#run_trips_panel_' + prev_run_id).replaceWith("<%= j render('dispatchers/run_trips_panel', run: @prev_run) %>");
    $('#runs_panel table tbody tr[data-run-id=' + prev_run_id + '] td.trips_count').html(<%= @prev_run.reload.trips.count%>);
  <% end %>

  <% if @need_to_update_trips_panel %>
    $('#unassigned_trips_panel').replaceWith("<%= j render('dispatchers/unassigned_trips_panel') %>");
    configure_unassigned_trips_table();
  <% else %>
    $('#unassigned_trips_panel .panel-title').html("<%= j render('dispatchers/unassigned_trips_panel_title') %>");
  <% end%>
<% end %>